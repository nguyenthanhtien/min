    "axios": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "localforage": "^1.10.0",
    "lodash-es": "^4.17.21",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.46.1",
    "react-paginate": "^8.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.13.0",
    "react-switch": "^7.0.0",
